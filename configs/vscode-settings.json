{
    "editor.cursorStyle": "block-outline",
    "editor.guides.bracketPairs": true,
    "editor.guides.bracketPairsHorizontal": true,
    "editor.wordWrap": "on",
    "editor.fontFamily": "Source Code Pro , 'Consolas'",
    "liveServer.settings.donotShowInfoMsg": true,
    "liveServer.settings.donotVerifyTags": true,
    "indentRainbow.errorColor": "rgba(128,32,32,0.6);",
    "npm.keybindingsChangedWarningShown": true,
    "tabnine.experimentalAutoImports": true,
    "java.configuration.runtimes": [],
    "redhat.telemetry.enabled": true,
    "C_Cpp.default.compilerPath": "C:\\Program Files (x86)\\Dev-Cpp\\MinGW64\\bin\\gcc.exe",
    "[python]": {
        "editor.formatOnType": true
    },
    "microprofile.alwaysShowOverview": false,
    "sonarlint.rules": {
        "Web:TableWithoutCaptionCheck": {
            "level": "off"
        },
        "Web:TableHeaderHasIdOrScopeCheck": {
            "level": "off"
        }
    },
    "workbench.iconTheme": "vscode-icons",
    "cSpell.userWords": [
        "ampm",
        "Arti",
        "cgpa",
        "choosen",
        "hloan",
        "Jiya",
        "Khandelwal",
        "Madhav",
        "Mahak",
        "manish",
        "memberof",
        "regdate",
        "Saini",
        "secretkey",
        "Skyblue",
        "Sneha",
        "Transformative",
        "tsup",
        "Vedansh",
        "vloan"
    ],
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "editor.fontSize": 16,
    "explorer.confirmDelete": false,
    "php.debug.executablePath": "",
    "vscode-edge-devtools.webhint": false,
    "hexeditor.columnWidth": 32,
    "hexeditor.showDecodedText": true,
    "hexeditor.defaultEndianness": "little",
    "hexeditor.inspectorType": "aside",
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "liveServer.settings.CustomBrowser": "chrome",
    "[css]": {
        "editor.defaultFormatter": "michelemelluso.code-beautifier"
    },
    "git.openRepositoryInParentFolders": "never",
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[php]": {
        "editor.defaultFormatter": "DEVSENSE.phptools-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "window.titleBarStyle": "custom",
    "terminal.integrated.defaultProfile.windows": "Command Prompt",
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[scss]": {
        "editor.defaultFormatter": "michelemelluso.code-beautifier"
    },
    "editor.cursorBlinking": "smooth",
    "editor.cursorSmoothCaretAnimation": "on",
    "background.enabled": false,
    "background.fullscreen": {

        "image": "",
        "images": [],
        "opacity": 0.91,
        "size": "cover",
        "position": "center",
        "interval": 0
    },
    "security.workspace.trust.untrustedFiles": "open",
    "indentRainbow.indentSetter": {},
    "rapidapi.terminalLink.enabled": false,
    "debug.onTaskErrors": "debugAnyway",
    "react-native-tools.showUserTips": false,
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "terminal.integrated.env.windows": {},
    "terminal.integrated.env.linux": {},
    "console-ninja.toolsToEnableSupportAutomaticallyFor": {
        "live-server-extension": true
    },
    "console-ninja.featureSet": "Community",
    "workbench.colorTheme": "One Dark Pro Darker",
    "emmet.includeLanguages": {
        "javascript": "javascriptreact"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[sass]": {
        "editor.defaultFormatter": "michelemelluso.code-beautifier"
    },
    "vsicons.dontShowNewVersionMessage": true,
    "[mdx]": {
        "editor.defaultFormatter": "unifiedjs.vscode-mdx"
    },
    "githubPullRequests.createOnPublishBranch": "never",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "githubPullRequests.pullBranch": "never",
    "peacock.favoriteColors": [

        {
            "name": "Angular Red",
            "value": "#dd0531"
        },
        {
            "name": "Azure Blue",
            "value": "#007fff"
        },
        {
            "name": "JavaScript Yellow",
            "value": "#f9e64f"
        },
        {
            "name": "Mandalorian Blue",
            "value": "#1857a4"
        },
        {
            "name": "Node Green",
            "value": "#215732"
        },
        {
            "name": "React Blue",
            "value": "#61dafb"
        },
        {
            "name": "Something Different",
            "value": "#832561"
        },
        {
            "name": "Svelte Orange",
            "value": "#ff3d00"
        },
        {
            "name": "Vue Green",
            "value": "#42b883"
        }
    ],
    "peacock.affectTabActiveBorder": true,
    "peacock.affectSideBarBorder": true,
    "peacock.affectPanelBorder": true,
    "peacock.surpriseMeOnStartup": true,
    "[markdown]": {
        "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
    },
    "codeQL.telemetry.enableTelemetry": false,
    "bootstrapIntelliSense.version": "Bootstrap v5.3",
    "hediet.vscode-drawio.resizeImages": null,
    "hediet.vscode-drawio.codeLinkActivated": true,
    "gitlens.advanced.messages": {
        "suppressLineUncommittedWarning": true
    },
    "git.ignoreRebaseWarning": true,
}
